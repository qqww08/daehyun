{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "lib": [
      "esnext",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictBindCallApply": true,
    "rootDir": "./",
    "strict": true,
    "target": "es2020",
    "baseUrl": ".",
    "noEmitOnError": true,
    "paths": {
      "~/*": [
        "*"
      ],
      "interface": [
        "interface/*"
      ],
      "pages": [
        "pages/*"
      ],
      "store": [
        "store/*"
      ],
      "styles": [
        "styles/*"
      ],
      "utils": [
        "utils/*"
      ],
      "hoc": [
        "hoc/*"
      ],
      "image": [
        "public/*"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
